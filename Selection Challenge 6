age = 13
ticket = True
if age >= 15 and ticket == True:
  print("Welcome to the cinema")
  print("Please find your seat")
elif age < 15:
  print("This movie is rated 15")
  print("You are not allowed this time")
else:
  print("You are not allowed this time")
  


